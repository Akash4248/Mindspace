# MindSpace AR Environment Variables

# API Configuration
REACT_APP_API_URL=http://localhost:3001/api
REACT_APP_ENV=development

# Authentication
REACT_APP_JWT_SECRET=your-jwt-secret-key
REACT_APP_OAUTH_GOOGLE_CLIENT_ID=your-google-client-id

# Third-party Services
REACT_APP_ANALYTICS_ID=your-analytics-id
REACT_APP_SENTRY_DSN=your-sentry-dsn

# Payment Processing (for premium features)
REACT_APP_STRIPE_PUBLISHABLE_KEY=your-stripe-key

# Feature Flags
REACT_APP_ENABLE_BIOMETRIC=true
REACT_APP_ENABLE_SOCIAL_FEATURES=true
REACT_APP_ENABLE_AR_FEATURES=false

# Development
REACT_APP_DEBUG_MODE=true
REACT_APP_MOCK_API=true
